# Maintainer: vitamin.caig@gmail.com
pkgname=zxtune123
pkgdesc="Commandline version of crossplatform ZX Spectrum music player and ripper"
url="https://zxtune.bitbucket.io/"
license=('GPL3')
optdepends=(
 'alsa-lib: for ALSA output support'
 'sdl: for SDL output support'
 'lame: for conversion to .mp3 format'
 'libvorbis: for conversion to .ogg format'
 'flac: for conversion to .flac format'
 'curl: for accessing files via network protocols'
)
conflicts=('zxtune')
